{
  "expo": {
    "scheme": "myapp", 
    "deepLinking": true,
    "name": "Eco Quest",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chatbot.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/chatbot.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chatbot.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
       "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "ecoquest.app"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/chatbot.png"
    }
  }
}
